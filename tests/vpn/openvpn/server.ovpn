tls-server

port 1194
proto udp

script-security 2

tls-verify ./vpn-tls-final.py

keying-material-exporter "EXPORTER_AAA" 20

# AAA Authority configuration
setenv aaa_handler   /usr/local/bin/aducidr3
setenv aaa_authority orange.alucid.eu

plugin /usr/local/lib/libaaa_vpn.so

reneg-sec 0
duplicate-cn
client-cert-not-required

dev tun

ca ca/cacert.pem
cert ca/server.cert.pem
key ca/server.key.pem

dh dh1024.pem

server 10.8.0.0 255.255.255.0

ifconfig-pool-persist ipp.txt

duplicate-cn

keepalive 10 120

persist-key
persist-tun

status openvpn-status.log

verb 4

